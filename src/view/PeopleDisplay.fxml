<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="800.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerPeopleDisplay">
   <children>
      <LineChart fx:id="lineChart" animated="false" legendVisible="false" prefHeight="337.0" prefWidth="780.0" title="Diagram">
         <xAxis>
            <CategoryAxis animated="false" label="time" />
         </xAxis>
         <yAxis>
            <NumberAxis animated="false" autoRanging="false" label="cm" minorTickCount="3" side="LEFT" tickUnit="20.0" upperBound="300.0" />
         </yAxis>
      </LineChart>
      <HBox spacing="10.0">
         <children>
            <Label text="People:" />
            <Label fx:id="labelPeopleNumber" text="null" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
